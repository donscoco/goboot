{
  "core": {
    "log": {
      "logPath": "/tmp/logpath",
      "logLevel": "INFO",
      "logMode": "day"
    },
    "dbManager": {
      "mysql": [{
        "proxyname": "ironhead-mysql",
        "username": "ironhead",
        "password": "${IRONHEAD_PWD}",
        "addr": "192.168.2.160:3306",
        "database": "test",
        "ConnMaxLifetime":7200
      }],
      "redis": [
        {
          "proxyname": "ironhead-redis",
          "username": "ironhead",
          "password": "",
          "addrs": ["192.168.2.128:6379"],
          "database": 0,
          "DialTimeout": 10,
          "ReadTimeout":30,
          "WriteTimeout":30,
          "MaxRetries":   3
        }
      ],
      "mongodb": [
        {
          "proxyname": "ironhead-mongodb",
          "username": "ironhead",
          "password": "${IRONHEAD_PWD}",
          "addrs":  [
            "192.168.2.128:27017",
            "192.168.2.128:27018",
            "192.168.2.128:27019"
          ],
          "database":  "ironhead",
          "ReplicaSetName": "ironhead",
          "timeout": 5
      }
      ]
    },
    "mq": {
      "redis": {
        "topic": "ironhead-mq-redis",
        "username": "ironhead",
        "password": "",
        "addr": "192.168.2.128:6379",
        "database": 0,
        "DialTimeout": 10,
        "ReadTimeout":30,
        "WriteTimeout":30,
        "MaxRetries":   3
      },
      "kafka": {
        "producer-config": {
          "name": "ironhead-prod",
          "Brokers": ["192.168.2.128:9092","192.168.2.128:9093","192.168.2.128:9094"],
          "SASL": {
            "Enable": false,
            "User": "",
            "Password": ""
          },
          "SetLog": true,
          "ReturnSuc": true,
          "RequiredAcks":"WaitForAll",
          "Version": "0.11.0.1"
        },
        "consumer-config": {
          "name": "ironhead-prod",
          "Brokers": ["192.168.2.128:9092","192.168.2.128:9093","192.168.2.128:9094"],
          "SASL": {
            "Enable": false,
            "User": "",
            "Password": ""
          },
          "GroupId": "go-dev",
          "Topics":["domark-test"],
          "SetLog": true,
          "AutoCommit": {
            "Enable": true,
            "Interval": 10
          },
          "Version": "0.11.0.1"
        }
       }
    }
  },
  "ServerObj": {
  }
}