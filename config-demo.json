{
  "core": {
    "log": {
      "logPath": "/tmp/logpath",
      "logLevel": "INFO",
      "logMode": "day"
    },
    "dbManager": {
      "mysql": [{
        "proxyname": "ironhead-mysql",
        "username": "ironhead",
        "password": "${IRONHEAD_PWD}",
        "addr": "192.168.2.160:3306",
        "database": "test",
        "ConnMaxLifetime":7200
      }],
      "redis": [
        {
          "proxyname": "ironhead-redis",
          "username": "ironhead",
          "password": "",
          "addrs": ["192.168.2.128:6379"],
          "database": 0,
          "DialTimeout": 10,
          "ReadTimeout":30,
          "WriteTimeout":30,
          "MaxRetries":   3
        }
      ],
      "mongodb": [
        {
          "proxyname": "ironhead-mongodb",
          "username": "ironhead",
          "password": "${IRONHEAD_PWD}",
          "addrs":  [
            "192.168.2.128:27017",
            "192.168.2.128:27018",
            "192.168.2.128:27019"
          ],
          "database":  "ironhead",
          "ReplicaSetName": "ironhead",
          "timeout": 5
      }
      ]
    },
    "mq": {
      "redis": {
        "topic": "ironhead-mq-redis",
        "username": "ironhead",
        "password": "",
        "addr": "192.168.2.128:6379",
        "database": 0,
        "DialTimeout": 10,
        "ReadTimeout":30,
        "WriteTimeout":30,
        "MaxRetries":   3
      },
      "kafka": {
        "producer-config": {
          "name": "ironhead-prod",
          "Brokers": ["192.168.2.128:9092","192.168.2.128:9093","192.168.2.128:9094"],
          "SASL": {
            "Enable": false,
            "User": "",
            "Password": ""
          },
          "SetLog": true,
          "ReturnSuc": true,
          "RequiredAcks":"WaitForAll",
          "Version": "0.11.0.1"
        },
        "consumer-config": {
          "ClientId": "ironhead-c2",
          "name": "ironhead-cons",
          "Brokers": ["192.168.2.128:9092","192.168.2.128:9093","192.168.2.128:9094"],
          "SASL": {
            "Enable": false,
            "User": "",
            "Password": ""
          },
          "GroupId": "go-dev",
          "Topics":["domark-test"],
          "SetLog": true,
          "AutoCommit": {
            "Enable": true,
            "Interval": 10
          },
          "Version": "0.11.0.1"
        }
       }
    },
    "coordinator": {
      "zookeeper": {
        "Addrs"   :["192.168.2.128:2181","192.168.2.128:2182","192.168.2.128:2183"],
        "Timeout": 6,
        "Path": "/ironhead"
      },
      "etcd": {
      }
    },
    "rpc": {
      "server":{
        "Addr": ":9090",
        "Node": "/rpc_server",
        "Prefix": "",
        "HeartbeatSec": 2
      },
      "client": {
        "Node": "/rpc_server",
        "MaxConns": 100,
        "MinConns": 10,
        "CallTimeoutMs"  :0,
        "DialTimeoutMs" : 6000,
        "Retries": 3
      }
    },
    "lock": {
      "redis": {
        "username": "ironhead",
        "password": "",
        "addrs": ["127.0.0.1:6379"],
        "database": 0,
        "DialTimeout": 10,
        "ReadTimeout":30,
        "WriteTimeout":30,
        "MaxRetries":   3
      },
      "zk": []
    }
  },
  "ServerObj": {
  }
}